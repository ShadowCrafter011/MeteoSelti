{
    "success": true,
    "data": {
        <% @measurements.each_with_index do |m, i| %>
            <% next unless m.sky_capture.attached? %>
            "<%= m.id %>": {
                "timestamp": <%= m.measured_at.to_i %>,
                "url": "<%= rails_storage_proxy_url m.sky_capture %>"
            }<%= "," unless i >= @measurements.length - 1 %>
        <% end %>
    }
}