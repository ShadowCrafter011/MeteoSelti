<div class="container">
    <h1><%= link_to "Flaticon", "https://www.flaticon.com/" %></h1>
    <table class="table">
        <thead>
            <tr>
                <th scope="col"><%= tp "author" %></th>
                <th scope="col"><%= tp "icon_set" %></th>
                <th scope="col"><%= tp "link" %></th>
            </tr>
        </thead>
        <tbody>
            <% @flaticon.each do |data| %>
                <%= render partial: "attribution", locals: data %>
            <% end %>
        </tbody>
    </table>
</div>